{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DomainProtect", 
      "Effect": "Allow",
      "Resource": "*",
      "Action": [
        "cloudfront:ListDistributions",
        "organizations:ListAccounts",
        "route53:ListHostedZones",
        "route53:ListResourceRecordSets",
        "route53domains:ListDomains"
      ]
    },
    {
      "Sid": "DomainProtectIPs", 
      "Effect": "Allow",
      "Resource": "*",
      "Action": [
        "ec2:DescribeAddresses",
        "ec2:DescribeRegions"
      ]
    }
  ]
}